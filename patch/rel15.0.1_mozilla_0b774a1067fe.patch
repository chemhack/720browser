+      snprintf(useff, MAXPATHLEN, "%s/chrome/.use-firefox-profile", container);
+      if (access(useff, F_OK) == 0)
+        snprintf(env, MAXPATHLEN, "XUL_APP_FILE=%s/application.ini", container);
+      else
+        snprintf(env, MAXPATHLEN, "XUL_APP_FILE=%s/720browser.ini", container);
+      putenv(env);
+    }
+  }
+#endif
