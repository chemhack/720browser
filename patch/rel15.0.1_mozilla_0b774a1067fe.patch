+#ifdef T20BRS
+  char container[MAXPATHLEN];
+  int ret;
+  pid_t pid; 
+  pid = getpid();
+  ret = proc_pidpath (pid, container, sizeof(container));
+  if (ret > 0) {
+    if (container[ret-1]=='x')
+      container[ret-7] = '\0'; // removing "firefox"
